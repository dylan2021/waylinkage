<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/main_fragments"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />

    <Button
        style="@style/titlesize_666"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dm050"
        android:background="@null"
        android:drawableLeft="@drawable/ic_back_arrow"
        android:drawablePadding="@dimen/dm020"
        android:onClick="onContractBackClick"
        android:padding="@dimen/title_left_bt_padding_left" />
</RelativeLayout>
